<!DOCTYPE html>
<html>
  <head>
    <title>MyCitiesocial backend</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="turbolinks-cache-control" content="no-cache">

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application_bd', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "shared/flash" if notice %>
    <%= render "shared/backend/nav_bar" %>
    <%= yield %>
  </body>
</html>
